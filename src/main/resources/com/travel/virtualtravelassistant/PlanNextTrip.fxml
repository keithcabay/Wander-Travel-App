<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.travel.virtualtravelassistant.PlanNextTrip.PlanNextTripController">
   <stylesheets>
   </stylesheets>
   <left>
      <fx:include maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="NavBar.fxml" />
   </left>
   <center>
      <VBox prefHeight="768.0" prefWidth="1151.0" spacing="20.0" BorderPane.alignment="CENTER">
         <HBox prefHeight="54.0" prefWidth="1166.0">
            <children>
               <ImageView fx:id="imageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                  <HBox.margin>
                     <Insets left="10.0" />
                  </HBox.margin></ImageView>
            </children>
            
         </HBox>
         <VBox alignment="TOP_CENTER" maxWidth="-Infinity" prefHeight="131.0" prefWidth="283.0" spacing="25.0" style="-fx-background-color: #385170; -fx-border-color: #385170;" translateX="10.0">
            <children>
               <TextField fx:id="locationTextField" promptText="Where would you like to go?" style="-fx-border-color: #385170;">
                  <font>
                     <Font size="20.0" />
                  </font>
               </TextField>
               <Button fx:id="letsGoButton" mnemonicParsing="false" onAction="#searchLocation" text="Let's Go!!!" textAlignment="CENTER">
                  <font>
                     <Font size="20.0" />
                  </font>
                  <VBox.margin>
                     <Insets bottom="20.0" />
                  </VBox.margin>
               </Button>
            </children>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </VBox>
         <Pane prefHeight="485.0" prefWidth="1157.0">
            <children>
               <VBox prefHeight="436.0" prefWidth="963.0" spacing="20">
                  <HBox prefHeight="436.0" prefWidth="959.0">
                     <children>
                        <TextArea fx:id="locationDetailsTextArea" editable="false" prefHeight="436.0" prefWidth="663.0" wrapText="true">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </TextArea>
                        <ScrollPane fx:id="imagesScrollPane" prefHeight="436.0" prefWidth="299.0">
                           <AnchorPane prefHeight="434.0" prefWidth="298.0">
                              <VBox fx:id="photosContainer" alignment="CENTER" layoutX="-12.0" prefHeight="434.0" prefWidth="300.0" spacing="10">
                                 <padding>
                                    <Insets bottom="10.0" />
                                 </padding>
                              </VBox>
                           </AnchorPane>
                        </ScrollPane>
                     </children>
                  </HBox>
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
               </VBox>
            </children>
         </Pane>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </center>
   <right>
      <Pane fx:id="PaneForBryan" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #385170;" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
