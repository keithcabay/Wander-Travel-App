<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="768.0" prefWidth="200.0" stylesheets="@NavBar.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.travel.virtualtravelassistant.NavBarController">
   <children>
      <AnchorPane prefHeight="768.0" prefWidth="200.0" styleClass="subMainContainer" stylesheets="@NavBar.css">
         <children>
            <VBox prefHeight="125.0" prefWidth="200.0" styleClass="vbox-top" stylesheets="@NavBar.css">
               <children>
                  <HBox prefHeight="50.0" prefWidth="200.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </HBox>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" prefWidth="200.0" text="WANDER" textAlignment="CENTER">
